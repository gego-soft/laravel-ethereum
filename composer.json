{
    "name": "gegosoft/laravel-ethereum",
    "description": "the ethereum json rpc api",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Ethereum",
        "Laravel"
    ],
    "authors": [
        {
            "name": "karthick",
            "email": "karthick@gegosoft.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "~5.3|~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Gegosoft\\LaravelEthereum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gegosoft\\LaravelEthereum\\EthereumServiceProvider"
            ],
            "aliases": {
                "Ethereum": "Gegosoft\\LaravelEthereum\\Facade\\Ethereum"
            }
        }
    }
}
